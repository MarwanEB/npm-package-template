{
  "name": "npm-template-with-tsup",
  "version": "0.0.3",
  "description": "npm package template with tsup and typescript",
  "scripts": {
    "build": "tsup ./src"
  },
  "homepage": "",
  "keywords": ["tsup", "npm", "template", "typescript"],
  "author": "koji",
  "license": "ISC",
  "devDependencies": {
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "files": ["dist", "package.json"],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts"
}
